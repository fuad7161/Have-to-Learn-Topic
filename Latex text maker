\section{mathematical equation}

	\begin{center}
			$(a+b)^2$= $a^2+2ab+b^2$\\
			{\large $h_2$O}
			
			$$\frac{a}{3}$$		
			$$\frac{\frac{a}{b}+d}{\frac{d}{e}+e}$$
			
			$$\frac{\sqrt{{b}^2+d}}{b}$$

			
			 $$\sqrt[a]{3}$$
			$$\sum_{x=1}^5 Y^Z$$
			$$\int_a^b f(x)$$

			$\alpha,\beta,\delta,\Delta, \theta,\Theta, \mu$\\

			$ax^2+bx+c=0$ \[x = \frac{-b\pm\sqrt{b^2-4ab}}{2a}\]
			\end{center}
		\begin{equation}
			\frac{a^2+b^2}{a+b} = a-b
		\end{equation}
		
		\begin{eqnarray}
			(a+b)(a+b) & = & a^2+ab+ba+b^2 					\nonumber\\
		& = & a^2+2ab+b^2
		\end{eqnarray}
		
		\begin{eqnarray}
			(a+b)(a+b) & = & a^2+ab+ab+b^2
			\nonumber\\
			& = & a^2+2ab+b^2			
		\end{eqnarray}
			\[
		\begin{array}{ccc}
		x-\lambda & 1 & 0 \\
		0 & x-\lambda & 1 \\
		0 & 0 & x-\lambda
		\end{array}
			\]
			\subsection{matrix}
			\[
		\begin{array}{ccc}
		x-\lambda & 1 & 0 \\
		0 & x-\lambda & 1 \\
		0 & 1 & x-\lambda
		
		\end{array}					
			\]
